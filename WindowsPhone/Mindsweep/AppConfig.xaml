   xmlns:shell="clr-namespace:Microsoft.Phone.Shell;assembly=Microsoft.Phone"
   xmlns:sys="clr-namespace:System;assembly=mscorlib"
   mc:Ignorable="d">


        <sys:String x:Key="AppTitleUppercase">MINDSWEEP</sys:String>
        <sys:String x:Key="AppTitle">Mindsweep</sys:String>
        <sys:String x:Key="AppTitleVersion">Mindsweep 1.0</sys:String>
